CREATE FUNCTION public.delete_shopping_cart_entry() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    UPDATE shoppingcart
	SET p_id_list = '{}'::smallint[]
    WHERE p_id_list = NEW.p_id_list
	AND customerid = NEW.customerid;
    RETURN NEW;
END;
$$;


ALTER FUNCTION public.delete_shopping_cart_entry() OWNER TO postgres;

--
-- TOC entry 295 (class 1255 OID 16787)
-- Name: update_archive(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.update_archive() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
	DELETE FROM inventory WHERE productid = OLD.p_id;
	INSERT INTO archive(p_id, p_name, color, description, category, subcategory, c_sizes, brand, price)
	VALUES(OLD.p_id, OLD.p_name, OLD.color, OLD.description, OLD.category, OLD.subcategory, OLD.c_sizes, OLD.brand, OLD.price);
	RETURN OLD;
END;
$$;


ALTER FUNCTION public.update_archive() OWNER TO postgres;

--
-- TOC entry 293 (class 1255 OID 16514)
-- Name: update_inventory(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.update_inventory() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE

value varchar;
p_idList smallint[];
product_id smallint;
quantList smallint[];
BEGIN
SELECT p_id_list INTO p_idList FROM shoppingcart WHERE customerid = NEW.customerid;
  FOREACH product_id IN ARRAY(p_idList)
  LOOP
  	SELECT cart->CAST(product_id as VARCHAR) INTO value FROM shoppingcart WHERE customerid = NEW.customerid;
    UPDATE inventory SET o_stock = o_stock - CAST(value as INTEGER) WHERE productid = product_id;
	UPDATE ordr SET quant_list = quant_list || CAST(value as SMALLINT) WHERE customerid = NEW.customerid; 
  END LOOP;
  RETURN NEW;
END;

$$;


ALTER FUNCTION public.update_inventory() OWNER TO postgres;

